# used to test cookie.py

from cookie import Jar

def test_init():
    ...
    
    
def test_str():
    jar = Jar()
    assert str(jar) == ""
    jar.depost(1)
    assert str(jar) == "🍪"
    jar.deposit(11)
    assert str(jar) == "🍪🍪🍪🍪🍪🍪🍪🍪🍪🍪🍪🍪"
    
